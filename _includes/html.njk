<!doctype html>
<html>
  <head>
    <title>{{ site.title }}</title>
    <meta charset="utf8" />
    <meta name="viewport" content="width=device-width" />
    <!-- @openlab/alembic inject-css -->
    <link rel="stylesheet" href="https://fonts.openlab.dev/inter/inter.css" />
    <link rel="stylesheet" href="assets/style.css" />
  </head>
  <body>
    <header class="hero">
      <img class="hero-background" src="assets/hero.jpg" />
      <!-- <img class="hero-brand" src="assets/brand.png" alt="NortHFutures" /> -->
      <div class="hero-brand" side="right">
        <center-layout gutters="var(--s0)">
          <cluster-layout
            justify="space-between"
            align="center"
            space="var(--s-1)"
          >
            <!-- <img alt="NortHFutures" src="assets/wordmark.png" width="300" /> -->
            <p class="wordmark">NortHFutures</p>
            <img alt="EPSRC" src="assets/epsrc.png" width="200" />
          </cluster-layout>
        </center-layout>
      </div>
    </header>
    <main>
      <center-layout> {{ content | safe }} </center-layout>
    </main>
    <footer> {{ site.title }} 2023 </footer>
    <!-- @openlab/alembic inject-js -->
  </body>
</html>
